{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "Number set",
    "type": "array",
    "items": {
        "title": "Number",
        "type": "object",
        "properties": {
            "apiVersion": {
                "type": "string"
            },
            "kind": {
                "type": "string",
                "readOnly": true
            },
            "metadata": {
                "type": "object",
                "properties": {
                    "ref": {"type": "string", "readOnly": true},
                    "gwRef": {"type": "string", "readOnly": true}
                },
                "required": ["gwRef"]
            },
            "spec": {
                "type": "object",
                "properties": {
                    "location": {
                        "type": "object",
                        "properties": {
                            "telUrl": {"type": "string"},
                            "aorLink": {"type": "string"}
                        },
                        "required": ["telUrl", "aorLink"]
                    }
                },
                "required": ["location"]
            }
        },
        "required": ["apiVersion", "kind", "metadata", "spec"]
    }
}
