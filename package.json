{
  "name": "sipio",
  "version": "1.0.0",
  "description": "Next generation proxy server",
  "scripts": {
    "transpile": "./node_modules/.bin/babel --out-dir node_modules mod",
    "bundle": "./node_modules/.bin/webpack --config webpack.config.js",
    "test": "./etc/tests/tester 'utils/test' 'core/test' 'core/acl/test' 'core/processor/test' 'registry/test' 'registrar/test' 'location/test' 'rest/test' 'data_api/test_redis_datasource' 'data_api/test_files_datasource' 'data_api/test_restful_datasource'",
    "pack": "./pack.sh",
    "prebundle": "mkdir -p libs && wget -nc https://raw.githubusercontent.com/nodyn/jvm-npm/master/src/main/javascript/jvm-npm.js && mv jvm-npm.js libs && gradle shadowJar && npm run transpile",
    "pretest": "npm run bundle",
    "prepack": "npm test"
  },
  "dependencies": {
    "contracts-es6": "^3.0.0",
    "jvm-npm": "^0.1.1",
    "moment": "^2.18.1"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-es2015": "6.24.1",
    "webpack": "^2.6.1"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/fonoster/sipio.git"
  },
  "license": "MIT"
}
